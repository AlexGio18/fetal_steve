<%- model_class = @gallery.class -%>
<div class="page-header">
  <h1 class="text-center">
    <%= link_to tour_path(@tour.id) do %>
      <i class="glyphicon glyphicon-log-out back-icon" title="Back To <%=@tour.name%>"></i>
    <% end %>
    <%=t '.title', :default => t('helpers.titles.edit', :model => model_class.model_name.human,
                                   :default => "Edit #{model_class.model_name.human}") %></h1>
</div>
<%= render :partial => 'form' %>