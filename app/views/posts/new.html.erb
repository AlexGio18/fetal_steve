<h3>New Placenta</h3>
<%= form_for(@post, html: { multipart: true}) do |f| %>
 <ul>
  <% @post.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %><br />
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %><br />
  </p>
  <p>
    <%= f.submit %>
  </p>
  <% end %>
<%= link_to "<< Back to Placenta", posts_path %>